<launch>
  <!-- No namespace here as we will share this description. 
       Access with slash at the beginning -->
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find bioloid_typea_description)/typea.xacro'" />
 
  <!-- BEGIN ROBOT 1-->
  <group ns="nelso">
    <param name="tf_prefix" value="nelso_tf" />
    <include file="$(find bioloid_config)/launch/bioloid_spawn_one_robot.launch" >
      <arg name="init_pose" value="-x 1 -y 1 -z 0.3" />
      <arg name="robot_name"  value="Nelso" />
    </include>

    <include file="$(find bioloid_config)/launch/bioloid_controllers_multibot.launch">
        <arg name="name" value="nelso"/>
    </include>
    
    <rosparam file="$(find bioloid_motion)/config/walk.yaml" command="load"/>
    <!-- Launch Walker Script -->
    <node name="typea_walker" pkg="bioloid_motion" type="robot_motion_walk" output="screen"/>
  </group>
 
  <!-- BEGIN ROBOT 2-->
  <!--<group ns="robot2">
    <param name="tf_prefix" value="robot2_tf" />
    <include file="$(find bioloid_config)/launch/bioloid_spawn_one_robot.launch" >
      <arg name="init_pose" value="-x -1 -y 1 -z 0.3" />
      <arg name="robot_name"  value="Robot2" />
    </include>
    <include file="$(find bioloid_config)/launch/bioloid_controllers_multibot.launch">
        <arg name="name" value="robot2"/>
    </include>
    <rosparam file="$(find bioloid_motion)/config/walk.yaml" command="load"/>
    
    <node name="typea_walker" pkg="bioloid_motion" type="robot_motion_walk" output="screen"/>
  </group> -->

</launch>