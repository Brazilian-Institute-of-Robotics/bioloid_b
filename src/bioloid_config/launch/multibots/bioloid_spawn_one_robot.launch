<launch>
    <arg name="robot_name"/>
    <arg name="init_pose"/>

    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
     args="$(arg init_pose) -urdf -param /robot_description -model $(arg robot_name)"
     respawn="false" output="screen" />

    <node pkg="robot_state_publisher" type="robot_state_publisher" 
        name="robot_state_publisher" output="screen"/>

    <rosparam param="source_list">['ax_joint_states']</rosparam>
    <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
        <remap from="/joint_state" to="/$(arg robot_name)/joint_state"/>
    </node>

    <node pkg="tf" type="static_transform_publisher" name="static_world_to_odom_publisher" args="0 0 0 0 0 0 $(arg robot_name)/world $(arg robot_name)/odom 100"/>
    <node pkg="tf" type="static_transform_publisher" name="static_odom_to_base_link_publisher" args="0 0 0.29 0 0 0 $(arg robot_name)/odom $(arg robot_name)/base_link 100"/>
</launch>